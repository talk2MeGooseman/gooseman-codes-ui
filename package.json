{
  "name": "gooseman-codes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "apollo-boost": "^0.4.9",
    "axios": "^0.20.0",
    "graphql": "^15.0.0",
    "loaders.css": "^0.1.2",
    "node-sass-chokidar": "^1.5.0",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.13.0",
    "react-alice-carousel": "^1.19.3",
    "react-dom": "^16.3.2",
    "react-icons": "^3.10.0",
    "react-loaders": "^3.0.1",
    "react-masonry-component": "^6.2.0",
    "react-scripts": "^3.4.3",
    "react-scroll": "^1.7.10",
    "react-twitch-embed-video": "^2.0.3",
    "serve": "^11.3.2",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.2.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "cross-env PORT=3200 react-scripts start",
    "start:dev": "npm-run-all -p watch-css start-js",
    "start": "react-scripts start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "build-and-deploy": "npm run build && aws s3 sync build/ s3://gooseman.codes",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint:css": "stylelint './src/**/*.js'"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^7.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
